{
    "name": "mdbulletcaps",
    "displayName": "MDBulletCaps",
    "description": "An extension that capitalizes bullet points inside of Markdown files.",
    "publisher": "timrolsh",
    "repository": {
        "type": "git",
        "url": "https://github.com/timrolsh/MDBulletCaps"
    },
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./extension.js",
    "scripts": {
        "lint": "eslint .",
        "pretest": "yarn run lint",
        "test": "vscode-test"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.capitalizeMarkdownList",
                "title": "Capitalize Markdown List Items"
            }
        ]
    },
    "devDependencies": {
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@types/vscode": "^1.85.0",
        "@vscode/test-cli": "^0.0.4",
        "@vscode/test-electron": "^2.3.8",
        "typescript": "^5.3.3"
    }
}

